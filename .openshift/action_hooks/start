#!/bin/bash
export PYTHON_EGG_CACHE=${OPENSHIFT_REPO_DIR}misc/virtenv/lib/python2.7/site-packages/
chmod +x ${OPENSHIFT_REPO_DIR}misc/virtenv/lib/python2.7/site-packages/thumbor
nohup ${OPENSHIFT_REPO_DIR}misc/virtenv/lib/python2.7/site-packages/thumbor -i ${OPENSHIFT_INTERNAL_IP} -p ${OPENSHIFT_INTERNAL_PORT} -c ${OPENSHIFT_REPO_DIR}thumbor.conf > ${OPENSHIFT_DIY_LOG_DIR}thumbor.log 2>&1 &
